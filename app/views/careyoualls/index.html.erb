<p id="notice"><%= notice %></p>

<h1>Careyoualls</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>A1</th>
      <th>B1</th>
      <th>C1</th>
      <th>D1</th>
      <th>E1</th>
      <th>F1</th>
      <th>G1</th>
      <th>H1</th>
      <th>I1</th>
      <th>J1</th>
      <th>K1</th>
      <th>L1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @careyoualls.each do |careyouall| %>
      <tr>
        <td><%= careyouall.name %></td>
        <td><%= careyouall.a1 %></td>
        <td><%= careyouall.b1 %></td>
        <td><%= careyouall.c1 %></td>
        <td><%= careyouall.d1 %></td>
        <td><%= careyouall.e1 %></td>
        <td><%= careyouall.f1 %></td>
        <td><%= careyouall.g1 %></td>
        <td><%= careyouall.h1 %></td>
        <td><%= careyouall.i1 %></td>
        <td><%= careyouall.j1 %></td>
        <td><%= careyouall.k1 %></td>
        <td><%= careyouall.l1 %></td>
        <td><%= link_to 'Show', careyouall %></td>
        <td><%= link_to 'Edit', edit_careyouall_path(careyouall) %></td>
        <td><%= link_to 'Destroy', careyouall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

